<Application x:Class="wallabag.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="using:wallabag">
    <Application.RequestedTheme>Light</Application.RequestedTheme>
    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="AppName">wallabag</x:String>
            <vm:ViewModelLocator x:Key="Locator" xmlns:vm="using:wallabag.ViewModel" />

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>